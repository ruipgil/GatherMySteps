{
  "name": "gathermysteps",
  "version": "1.0.0",
  "description": "Process personal tracks",
  "main": "index.js",
  "scripts": {
    "dev": "env NODE_ENV=development node server.js",
    "test": "mocha --compilers js:babel-register --recursive",
    "test:watch": "npm test -- --watch",
    "clean": "rm -r build || true",
    "build": "npm run build:gathermysteps",
    "build:gpxplorer": "npm run clean && env NODE_ENV=production BUILD_GPX=true webpack webpack.config.js build/ && cp src/html/*.html build",
    "build:gathermysteps": "webpack webpack.config.js build/ --progress && cp src/html/*.html build",
    "deploy:gpxplorer": "npm run clean && npm i --save-dev && npm run build:gpxplorer && cp .gitignore build && git checkout gh-pages && cp build/* . && cp build/.gitignore . && rm -r build && git add . && git commit -m 'Build '$(git log -n 1 master --pretty=format:%H) && git checkout master",
    "lint": "eslint src/**/*.js src/**/*.jsx"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ruipgil/gathermysteps.git"
  },
  "author": "Rui Gil",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ruipgil/gathermysteps/issues"
  },
  "homepage": "https://github.com/ruipgil/gathermysteps#readme",
  "dependencies": {
    "async": "^2.0.0-rc.4",
    "babel-polyfill": "^6.6.1",
    "bulma": "0.0.28",
    "deep-diff": "^0.3.4",
    "draft-js": "^0.7.0",
    "font-awesome": "^4.5.0",
    "gxparser": "^1.0.0",
    "haversine": "^1.0.0",
    "immutable": "^3.7.6",
    "isomorphic-fetch": "^2.2.1",
    "leaflet": "^1.0.0-rc.3",
    "leaflet-plugins": "github:ruipgil/leaflet-plugins",
    "moment": "^2.11.2",
    "pegjs": "^0.9.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.3.0",
    "react-slider": "^0.6.1",
    "redux": "^3.2.1",
    "redux-immutable": "^3.0.5",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^1.0.3",
    "thunk": "0.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-preset-stage-3": "^6.5.0",
    "babel-register": "^6.5.2",
    "css-loader": "^0.23.1",
    "eslint": "^2.0.0-rc.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-promise": "^1.0.8",
    "eslint-plugin-react": "^3.16.1",
    "eslint-plugin-standard": "^1.3.2",
    "expect": "^1.14.0",
    "express": "^4.13.4",
    "file-loader": "^0.8.5",
    "leaflet": "^1.0.0-rc.3",
    "mocha": "^2.4.5",
    "nock": "^7.2.2",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.4.0",
    "redux-devtools": "^3.1.0",
    "redux-mock-store": "0.0.6",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.6.4"
  }
}
